using System;

namespace Ejercicio19
{
    class Program
    {
        static void Main(string[] args)
        {
            int bucle;
            for (bucle = 1; bucle <= 100; bucle++)
            {
                if (bucle % 2 == 0 || bucle % 3 == 0)
                { Console.WriteLine(bucle); }
            }
            Console.ReadKey();
        }
    }
}
